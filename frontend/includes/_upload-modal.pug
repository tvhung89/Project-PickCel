.modal.fade.ads__upload.ads__display-add.show#upload-modal
    .modal-dialog.modal-md
        .modal-content
            .modal-header
                h3.modal-title Upload asset(s)
                a(href="#" data-dismiss="modal")
                    i.icon-close
            .modal-body.left
                form.d-none(action="#" method="post")
                    .form-group
                        input.form-control#fileUpload(type="file" name="fileUpload" multiple)
                        img(src="./images/file-upload.png" alt="File upload")
                .file-lists
                    .file   
                        .info
                            .left
                                img(src="data:image/png;base64," alt="Video Thumbnail")
                            .center
                                input(type="text" value="feature1")
                                p 95 KB
                            .right
                                p
                                    a(href="#")
                                        i.icon-trash
                        .progress
                            .progress-bar.progress-bar-striped(style="width: 25%") 25%
                    .file
                        .info
                            .left
                                img(src="data:image/png;base64," alt="Image Thumbnail")
                            .center
                                input(type="text" value="feature1")
                                p 95 KB
                            .right
                                p
                                    a(href="#")
                                        i.icon-trash
                        .progress
                            .progress-bar.progress-bar-striped(style="width: 47%") 47%
            .modal-footer
                button.btn.btn-success.rounded(type="button") Add files
                button.btn.btn-primary.rounded(type="button") Upload