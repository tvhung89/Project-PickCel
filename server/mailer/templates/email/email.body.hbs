<table width="100%" cellspacing="0" cellpadding="0" style="width: 100%;margin: 0;-webkit-text-size-adjust: none !important;font-family: sans-serif;">
    <tbody>
        <tr>
            <td>
                <table width="600" align="center" cellpadding="0" cellspacing="0"
                    style="background: #fff;padding: 16px;border: 1px solid #eee">
                    <tbody>
                        <tr>
                            <td>
                                <table bgcolor="#d0cfcf" class="banner" width="100%" align="center" cellpadding="0"
                                    cellspacing="0" style="width: 100%;background: url(https://signage.pickcel.com/email/banner.png) no-repeat;padding: 10% 0;height: 150px;background-size: cover;">
                                    <tbody>
                                        <tr>
                                            <td align="center"><img src="https://signage.pickcel.com/email/logo.png"
                                                    alt="" style="width: 200px;margin: 0 auto;"></td>
                                        </tr>
                                        <tr>
                                            <td
                                                style="font-weight: 500; font-size: 16px; color:#fff; text-align:center; font-family: sans-serif;">
                                                CLOUD DIGITAL SIGNAGE SOLUTION</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td style="font-size: 0; line-height: 0;" height="20">
                                <table width="96%" align="left" cellpadding="0" cellspacing="0">
                                    <tbody>
                                        <tr>
                                            <td style="font-size: 0; line-height: 0;" height="20">&nbsp;</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <table align="center" width="65%"
                                    style="border-top: 1px solid #eee;border-bottom: 1px solid #eee;padding: 5% 0px"
                                    cellpadding="0" cellspacing="0">
                                    <tbody>
                                        <tr>
                                            <td align="left"
                                                style="font-size: 16px; font-weight: 300; line-height: 2.5em; color: #929292; font-family: sans-serif;font-weight:bold;">
                                                Hi, <strong>{{email}}</strong></td>
                                        </tr>
                                        <tr>
                                            <td align="left"
                                                style="font-size: 14px; font-style: normal; font-weight: 100; color: #929292; line-height: 1.8; text-align:left; padding:10px 0; font-family: sans-serif;">
                                                Thanks for joining Pickcel, to complete the email verification please
                                                click on button below.</td>
                                        </tr>
                                        <tr>
                                            <td style="font-size: 0; line-height: 0;" height="60">
                                                <table width="96%" align="left" cellpadding="0" cellspacing="0">
                                                    <tbody>
                                                        <tr>
                                                            <td style="font-size: 0; line-height: 0;" height="40">&nbsp;
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </td>
                                        </tr>
                                        {{#if verifyUrl}}
                                        <tr>
                                            <td><a
                                                    href="{{verifyUrl}}" style="padding: 10px 20%;text-decoration: none;border: 1px solid #ff5252;color: #000;text-align: center;border-radius: 5px;margin-top: 4%;font-size: 14px;font-style: normal;font-weight: 100;">Verify
                                                    Email</a></td>
                                        </tr>
                                        {{/if}}
                                        <tr>
                                            <!-- HTML spacer row -->
                                            <td style="font-size: 0; line-height: 0;" height="20">
                                                <table width="96%" align="left" cellpadding="0" cellspacing="0">
                                                    <tbody>
                                                        <tr>
                                                            <td style="font-size: 0; line-height: 0;" height="40">&nbsp;
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <!-- HTML spacer row -->
                            <td style="font-size: 0; line-height: 0;" height="40">
                                <table width="96%" align="left" cellpadding="0" cellspacing="0">
                                    <tbody>
                                        <tr>
                                            <td style="font-size: 0; line-height: 0;" height="20">&nbsp;</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <table class="footer" width="25%" align="center" cellpadding="0" cellspacing="0">

                                    <!-- First column of footer content -->
                                    <tbody>
                                        <tr width="100%">
                                            <td width="30%">
                                                <img src="https://signage.pickcel.com/email/fb.png" alt="" style="margin: 0 auto;width: 34px;">
                                            </td>
                                            <td width="30%">
                                                <img src="https://signage.pickcel.com/email/in.png" alt="" style="margin: 0 auto;width: 34px;">
                                            </td>
                                            <td width="30%">
                                                <img src="https://signage.pickcel.com/email/twtr.png" alt="" style="margin: 0 auto;width: 34px;">
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                                <table class="footer" width="70%" align="center" cellpadding="0" cellspacing="0">
                                    <!-- Second column of footer content -->
                                    <tbody>
                                        <tr>
                                            <td>
                                                <p align="center"
                                                    style="font-size:12px; font-weight:300; line-height: 2.5em; color: #929292; font-family: sans-serif;">
                                                    Copyright Â© 2018 LaneSquare Technology Pvt. Ltd. | All Rights
                                                    Reserved</p>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </td>
        </tr>
    </tbody>
</table>