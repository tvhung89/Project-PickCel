  "repository": {
    "type": "git",
    "url": "https://github.com/hwebz/pickcel-react"
  },
  "homepage": "https://github.com/hwebz/pickcel-react",
  "main": "./dist/server.generated.js",
  "scripts": {
    "deploy": "npm run fe:prod && npm run build",
    "dev": "nodemon",
    "build": "webpack --config webpack.config.client.production.js && webpack --mode=production --config webpack.config.server.js",
    "start": "node server.generated.js",
    "fe:dev": "webpack --config=webpack.frontend.config.js",
    "fe:prod": "cross-env NODE_ENV=production webpack --config=webpack.frontend.config.js",
    "fe:watch": "webpack --config=webpack.frontend.config.js --watch"
  },
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ],
